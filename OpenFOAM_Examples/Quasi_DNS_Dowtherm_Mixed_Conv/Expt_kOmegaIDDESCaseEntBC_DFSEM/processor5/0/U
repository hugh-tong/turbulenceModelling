FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];


internalField   uniform (0 0 -0.22618);

boundaryField
{
    leftWall
    {
        type            noSlip;
    }
    rightWall
    {
        type            noSlip;
    }
    leftSides_periodic0
    {
        type            cyclic;
    }
    leftSides_periodic1
    {
        type            cyclic;
    }
    rightSides_periodic0
    {
        type            cyclic;
    }
    rightSides_periodic1
    {
        type            cyclic;
    }
    leftInOut_periodic0
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    leftInOut_periodic1
    {
        type            turbulentDFSEMInlet;
        value           uniform (0 0 -0.22618);
        delta           2;
        nCellPerEddy    1;
    }
    rightInOut_periodic0
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    rightInOut_periodic1
    {
        type            turbulentDFSEMInlet;
        value           uniform (0 0 -0.22618);
        delta           2;
        nCellPerEddy    1;
    }
    procBoundary5to2
    {
        type            processor;
        value           uniform (0 0 -0.22618);
    }
    procBoundary5to3throughleftSides_periodic0
    {
        type            processorCyclic;
        value           uniform (0 0 -0.22618);
    }
    procBoundary5to3throughrightSides_periodic0
    {
        type            processorCyclic;
        value           uniform (0 0 -0.22618);
    }
    procBoundary5to4
    {
        type            processor;
        value           uniform (0 0 -0.22618);
    }
}


